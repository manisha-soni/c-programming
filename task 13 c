#include<stdio.h>
main()
{
int a[100],smallest,largest,i,n;
printf("enter the array size");
scanf("%d",&n);
printf("enter array elements");
for(i=0;i<=n;i++)
{
scanf("%d",&a[i]);
}
a[0]=smallest;
a[0]=largest;
for(i=0;i<=n;i++)
{
if(a[i]<smallest)
{
a[i]=smallest;
}
if(a[i]>largest)
{
a[i]=largest;
}
}
printf("%d",smallest);
printf("%d",largest);
}
